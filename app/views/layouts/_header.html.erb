<header class ="navbar navbar-fixed-top">
	<div class="navbar-inner">
		<div class="container">	
			<%#= image_tag("HClogo.jpg", alt: "HumbleCasa.com logo", size: "40x15") %>
			<%= link_to "HumbleCasa", root_path, id: "logo" %>
			<nav>
				<ul class="nav pull-right">
					<% if current_user %>
						<%= link_to "Profile", edit_user_path(current_user) %> | 
						<%= link_to "Logout", logout_path %>
					<% else %>
						<%= link_to "Register", new_user_path(role: "propertyowner") %> | 
						<%= link_to "Login", login_path %>
					<% end %>
				</ul>
			</nav>
		</div>
	</div>
</header>