<%= form_for(@service) do |f| %>
	<p>
	    <%= f.label :service_business_name %><br />
	    <%= f.text_field :name %>
	</p>
	<p>
	    <%= f.label :address1 %><br />
	    <%= f.text_field :address1 %>
	</p>
	<p>
	    <%= f.label :address2 %><br />
	    <%= f.text_field :address2 %>
	</p>
	<p>
	    <%= f.label :city %><br />
	    <%= f.text_field :city %>
	</p>
	<p> 
	    <%= f.label :state %><br />
	    <%= f.text_field :state %>
	</p>
	<p>
	    <%= f.label :zip %><br />
	    <%= f.text_field :zip %>
	</p>
	<p> 
	    <%= f.label :phone %><br />
	    <%= f.text_field :phone %>
	</p>
	<p> 
	    <%= f.label :email %><br />
	    <%= f.text_field :email %>
	</p>
	<p><%= f.submit %></p>
<% end %>