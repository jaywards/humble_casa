<!-- Property details -->
<%= render property_listing %><br>
<z class="launch-assign-services">
	<%= link_to assign_services_property_path(property_listing), class: "btn btn-mini btn-primary" do %>
		<i class="icon-check icon-white"></i> Register or Change Services
	<% end %>
</z>
&nbsp
<span class="launch-property">
	<%= link_to edit_property_path(property_listing), class: "btn btn-mini" do%>
		<i class="icon-edit"></i> Edit Property
	<% end %>
</span>
&nbsp
<%= link_to property_listing, class: "btn btn-mini btn-inverse", method: :delete, confirm: "Are you sure?", title: property_listing.name do%>
	<i class="icon-trash icon-white"></i> Delete Property
<% end %>
<hr>
